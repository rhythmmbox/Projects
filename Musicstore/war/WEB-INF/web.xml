<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<sessions-enabled>true</sessions-enabled>
	
	<servlet>
		<servlet-name>Musicstore</servlet-name>
		<servlet-class>com.musicstore.MusicstoreServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Musicstore</servlet-name>
		<url-pattern>/musicstore</url-pattern>
	</servlet-mapping>
	<servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>com.musicstore.controller.SignupController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>Login</servlet-name>
    <servlet-class>com.musicstore.controller.LoginController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
	
	<servlet>
<servlet-name>Logout</servlet-name>
    <servlet-class>com.musicstore.controller.LogoutController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
<servlet-name>AddItem</servlet-name>
    <servlet-class>com.musicstore.controller.AddItemController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AddItem</servlet-name>
    <url-pattern>/additem</url-pattern>
  </servlet-mapping>
  
   <servlet>
<servlet-name>Order</servlet-name>
    <servlet-class>com.musicstore.controller.OrderController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Order</servlet-name>
    <url-pattern>/order</url-pattern>
  </servlet-mapping>
  
    <servlet>
<servlet-name>Photo</servlet-name>
    <servlet-class>com.musicstore.controller.PhotoUploadController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Photo</servlet-name>
    <url-pattern>/uploadphoto</url-pattern>
  </servlet-mapping>
  
   <servlet>
<servlet-name>Serve</servlet-name>
    <servlet-class>com.musicstore.controller.ServeController</servlet-class>
  </servlet>


  
  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/serve</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
	<welcome-file-list>
		<welcome-file>home.jsp</welcome-file>
	</welcome-file-list>
</web-app>
